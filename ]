#include < unistd.h >

voidft_putchar(char c)
{
	write(1, &c, 1);
}

voidft_print_alphabet(void)

{
char letter;
	letter = 'a';

while (letter <= 'z')
{
ft_putcher(letter);
		letter++
	}
}

int main(void)
{
	ft_print_alphabet();
	return 0;
}
